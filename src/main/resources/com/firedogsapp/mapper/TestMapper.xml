<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.firedogsapp.mapper.TestMapper">
	<select id = "test" resultType = "String">
		SELECT 
		company_name
		FROM FD_COMPANY
		LIMIT 1
	</select>
	
	<select id = "select_acccount" resultType = "String">
		SELECT
		user_id
		FROM FD_USER
		WHERE user_id = #{username} AND passwd = #{password}
	</select>
</mapper>